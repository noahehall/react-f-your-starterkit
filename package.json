  "repository": {
    "type": "git",
    "url": "https://github.com/noahehall/react-f-your-starterkit.git"
  },
  "scripts": {
    "ssr": "NODE_ENV=development babel-node -- ./ssr.js",
    "client": "NODE_ENV=development webpack-dev-server --config webpack.config.babel.js --env.platform=web --env.ssr=false --env.port=3000 --env.host=0.0.0.0 --debug --progress",
    "prodclient": "NODE_ENV=production webpack --config webpack.config.babel.js --env.platform=web --env.ssr=true --env.port=3000 --env.host=0.0.0.0 --progress",
    "prodserver": "NODE_ENV=production webpack --config webpack.config.babel.js --env.platform=node --env.ssr=true --env.port=3000 --env.host=0.0.0.0 --progress",
    "build": "del './dist/*' && yarn prodclient && yarn prodserver",
    "security": "./node_modules/.bin/nsp check",
    "eslint-unused": "./node_modules/.bin/eslint-find-rules -u ./.eslintrc.js -n",
    "eslint-fix": "./node_modules/.bin/eslint --fix \"./src/**/*.js\" --config ./.eslintrc.js",
    "stylefmt": "./node_modules/.bin/stylefmt -r src/**/*.css"
  },
  "engines": {
    "node": ">=8",
    "npm": " >=5"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "dependencies": {
    "antd": "^2.13.6",
    "axios": "^0.17.0",
