{
  "name": "react-f-your-starterkit",
  "version": "0.3.1",
  "description": "For all of your progressive reactjs starter kit needs",
  "keywords": [
    "reactjs",
    "starter kit",
    "PWA",
    "progressive web app",
    "security starter kit",
    "http2"
  ],
  "homepage": "https://github.com/noahehall/react-f-your-starterkit",
  "bugs": {
    "url": "https://github.com/noahehall/react-f-your-starterkit/issues",
    "email": "noahedwardhall@gmail.com"
  },
  "license": "MPL-2.0",
  "author": "noah edward hall <noahedwardhall@gmail.com> (https://twitter.com/noahedwardhall)",
  "repository": {
    "type": "git",
    "url": "https://github.com/noahehall/react-f-your-starterkit.git"
  },
  "scripts": {
    "start": "./node_modules/.bin/gulp",
    "security": "./node_modules/.bin/nsp check",
    "predev": "npm run stylefmt",
    "dev": "NODE_ENV=development && npm run start",
    "preprod": "NODE_ENV=production ./node_modules/.bin/gulp prod",
    "prod": "NODE_ENV=production node ./dist/server.js",
    "prebuild": "npm run eslint-fix",
    "build": "NODE_ENV=production ./node_modules/.bin/gulp prod",
    "flow": "./node_modules/.bin/flow; test $? -eq 0 -o $? -eq 2",
    "test": "./node_modules/.bin/gulp test",
    "eslint": "./node_modules/.bin/gulp eslint",
    "lint": "./node_modules/.bin/gulp lint",
    "eslint-unused": "./node_modules/.bin/eslint-find-rules -u ./.eslintrc -n",
    "eslint-fix": "./node_modules/.bin/eslint --fix \"./src/**/*.js\" --config ./.eslintrc.js",
    "stylefmt": "./node_modules/.bin/stylefmt -r src/**/*.css",
    "stylelint": "npm run stylefmt && ./node_modules/.bin/gulp stylelint"
  },
  "engines": {
    "node": ">=6",
    "npm": " >=4"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "dependencies": {
    "axios": "^0.15.2",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "helmet": "^3",
    "idb": "github:noahehall/idb#isomorphic-imports-2",
    "ionicons": "^3.0.0",
    "lodash": "^4.17.2",
    "moment": "^2.15.1",
    "moment-duration-format": "^1.3.0",
    "react": "^15.2.1",
    "react-dom": "^15.3.1",
    "react-helmet": "^3.1.0",
    "react-markdown": "^2.4.2",
    "react-modal": "^1.4.0",
    "react-popup": "^0.4.2",
    "react-proxy": "^1.1.8",
    "react-redux": "^4.4.5",
    "react-router": "^3",
    "reactabular-table": "^8.1.0",
    "redux": "^3.6.0",
    "redux-promise": "^0.5.3",
    "redux-seamless-immutable": "^0.3.0",
    "redux-thunk": "^2.1.0",
    "rollbar": "^0.6.3",
    "seamless-immutable": "^6.3.0",
    "spdy": "^3.4.4",
    "spotlight": "^1.2.0",
    "st-input-moment": "^0.3.3",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-plugin-module-resolver": "^2.2.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "browserify-postcss": "^0.3.2",
    "chai": "^3.5.0",
    "del": "^2.2.2",
    "enzyme": "^2.4.1",
    "eslint": "^3.5.0",
    "eslint-config-ct-fletcher": "^1.0.3",
    "eslint-find-rules": "^1.13.1",
    "event-stream": "^3.3.4",
    "flow-bin": "^0",
    "glob": "^7.1.1",
    "gulp": "^3.9.1",
    "gulp-copy": "0.0.2",
    "gulp-env": "^0.4.0",
    "gulp-eslint": "^3.0.1",
    "gulp-if": "^2.0.1",
    "gulp-nodemon": "^2.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sequence": "^0.4.6",
    "gulp-sourcemaps": "^2.1.1",
    "gulp-spawn-mocha": "^3.1.0",
    "gulp-stylelint": "^3.2.0",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7",
    "ignore-styles": "^5.0.1",
    "istanbul": "1.1.0-alpha.1",
    "jsdom": "^9.5.0",
    "livereactload": "latest",
    "loose-envify": "^1.2.0",
    "mocha": "^3.0.2",
    "nodemon": "^1.9.2",
    "nsp": "^2.6.2",
    "postcss-advanced-variables": "^1.2.2",
    "postcss-cssnext": "^2.7.0",
    "postcss-extend": "^1.0.5",
    "postcss-import": "^8.1.2",
    "postcss-reporter": "^2.0.0",
    "react-addons-test-utils": "^15.3.1",
    "redux-logger": "^2.7.0",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "stringify": "^5.1.0",
    "stylefmt": "github:morishitter/stylefmt",
    "stylelint-csstree-validator": "^1.0.3",
    "uglifyify": "^3.0.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
  }
}
